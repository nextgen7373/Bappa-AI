{
  "rewrites": [
    {
      "source": "/chat",
      "destination": "/index.html"
    },
    {
      "source": "/404",
      "destination": "/index.html"
    },
    {
      "source": "/((?!assets/).*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
